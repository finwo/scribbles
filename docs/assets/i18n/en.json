{
  "timeago": [
    { "limit":       60, "format": "just now" },
    { "limit":      120, "format": "1 minute ago" },
    { "limit":     3600, "format": "%m minutes ago" },
    { "limit":     7200, "format": "1 hour ago" },
    { "limit":    86400, "format": "%h hours ago" },
    { "limit":   172800, "format": "1 day ago" },
    { "limit":   604800, "format": "%d days ago" },
    { "limit": 12960000, "format": "%MM %Do" },
    { "limit":     null, "format": "%MM %Do, %Y" }
  ],
  "date": {
    "format": [
      {
        "symbol": "%MM",
        "source": "Month",
        "map"   : {
          "0": "January",
          "1": "February",
          "2": "March",
          "3": "April",
          "4": "May",
          "5": "June",
          "6": "July",
          "7": "August",
          "8": "September",
          "9": "October",
          "10": "November",
          "11": "December"
        }
      },
      {
        "symbol": "%Do",
        "source": "Date",
        "translate": "nth"
      },
      {
        "symbol": "%Y",
        "source": "FullYear"
      }
    ]
  }
}
